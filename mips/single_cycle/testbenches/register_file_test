    .scope S_0x839700;
T_9 ;
    %set/v v0x889f20_0, 1, 1;
    %movi 8, 32, 7;
T_9.0 %cmp/s 0, 8, 7;
    %jmp/0xz T_9.1, 5;
    %add 8, 1, 7;
    %vpi_func 2 81 "$random", 15, 32;
    %set/v v0x88a100_0, 15, 32;
    %load/v 15, v0x88a100_0, 32;
    %ix/getv 3, v0x88a050_0;
   %jmp/1 t_2, 4;
   %ix/load 1, 0, 0;
   %set/av v0x889fd0, 15, 32;
t_2 ;
    %delay 100, 0;
    %load/v 15, v0x88a050_0, 5;
    %mov 20, 0, 27;
    %addi 15, 1, 32;
    %set/v v0x88a050_0, 15, 5;
    %jmp T_9.0;
T_9.1 ;
    %set/v v0x889f20_0, 0, 1;
    %set/v v0x88a050_0, 0, 5;
    %set/v v0x88a100_0, 2, 32;
    %movi 8, 16, 6;
T_9.2 %cmp/s 0, 8, 6;
    %jmp/0xz T_9.3, 5;
    %add 8, 1, 6;
