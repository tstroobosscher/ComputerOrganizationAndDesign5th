   %cmpi/s 8, 32, 32;
    %jmp/0xz T_0.1, 5;
    %load/v 8, v0x889450_0, 32;
    %ix/getv/s 3, v0x889450_0;
   %jmp/1 t_0, 4;
   %ix/load 1, 0, 0;
   %set/av v0x889810, 8, 32;
t_0 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0x889450_0, 32;
    %set/v v0x889450_0, 8, 32;
    %jmp T_0.0;
T_0.1 ;
    %end;
