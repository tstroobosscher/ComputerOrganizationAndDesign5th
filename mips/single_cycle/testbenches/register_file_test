    %assign/av v0x889810, 0, 8;
t_1 ;
T_1.0 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x839700;
T_2 ;
    %delay 50, 0;
    %load/v 8, v0x889b40_0, 1;
    %inv 8, 1;
    %set/v v0x889b40_0, 8, 1;
